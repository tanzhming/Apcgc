<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.immense.tw.dao.AtypeDao">  

	<select id="getAllTypes" resultMap="typeMapper">
		select id,typename,typenickname from atype
	</select>

	<resultMap type="com.immense.tw.entity.Atype" id="typeMapper">
		<id property="id" column="id" />
		<result property="typename" column="typename" />
		<result property="typenickname" column="typenickname"/>
	</resultMap>
	
	
</mapper>














